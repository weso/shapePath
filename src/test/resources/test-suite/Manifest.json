      "expect": "1dotRefOR3_p1_valueExpr_type"
    },
    {
      "name": "1dotRefOR3_S1_p1_valueExpr_3",
      "from": "./1dotRefOR3.json",
      "shapePath": "/@<http://a.example/S1>/<http://a.example/p1>.valueExpr/@3",
      "expect": "1dotRefOR3_S4"
    },
    {
      "name": "1dotRefOR3_S1_p1_at_3",
